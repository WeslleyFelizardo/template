        "build-prod-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build -c prod --stats-json",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json"
